<div id="<%= dom_id item %>">
  <p>
    <strong>Artist:</strong>
    <%= item.artist %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= item.name %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= item.quantity %>
  </p>
  <p>
  <strong>warehouse:</strong>
    <%= link_to item.warehouse.name, item.warehouse %>
  </p>

</div>
